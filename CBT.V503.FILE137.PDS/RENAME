//TSSRENA  JOB (NAME),
//         NOTIFY=&SYSUID,
//         CLASS=A,MSGCLASS=X,MSGLEVEL=(1,1)
//******************************************************************
//*       THIS HAS THREE BASE STEPS                                *
//*   1)  DELETE THE OLD VERSIONS OF THESE FILES                   *
//*   2)  RENAME THE CURRENT VERSIONS TO BE "OLD"                  *
//*   3)  RENAME THE "NEW" FILES TO BE THE CURRENT FILES  *
//******************************************************************
//STEP1   EXEC PGM=IDCAMS
//SYSPRINT DD SYSOUT=*
//SYSIN  DD *

     DELETE    (OPT.CICS.OLD.REPORT.DETAIL.FILE)      -
               CLUSTER PURGE
     DELETE    (OPT.CICS.OLD.REPORT.INDEX.FILE)        -
               CLUSTER PURGE
     DELETE    (OPT.CICS.OLD.REPORT.HISTORY.FILE)      -
               CLUSTER PURGE

   ALTER OPT.CICS.REPORT.DETAIL.FILE    -
     NEWNAME(OPT.CICS.OLD.REPORT.DETAIL.FILE)
   ALTER OPT.CICS.REPORT.DETAIL.FILE.*  -
     NEWNAME(OPT.CICS.OLD.REPORT.DETAIL.FILE.*)

   ALTER OPT.CICS.REPORT.INDEX.FILE    -
     NEWNAME(OPT.CICS.OLD.REPORT.INDEX.FILE)
   ALTER OPT.CICS.REPORT.INDEX.FILE.*  -
     NEWNAME(OPT.CICS.OLD.REPORT.INDEX.FILE.*)

   ALTER OPT.CICS.REPORT.HISTORY.FILE    -
     NEWNAME(OPT.CICS.OLD.REPORT.HISTORY.FILE)
   ALTER OPT.CICS.REPORT.HISTORY.FILE.*  -
     NEWNAME(OPT.CICS.OLD.REPORT.HISTORY.FILE.*)

   ALTER OPT.CICS.NEW.REPORT.DETAIL.FILE    -
     NEWNAME(OPT.CICS.REPORT.DETAIL.FILE)
   ALTER OPT.CICS.NEW.REPORT.DETAIL.FILE.*  -
     NEWNAME(OPT.CICS.REPORT.DETAIL.FILE.*)

   ALTER OPT.CICS.NEW.REPORT.INDEX.FILE    -
     NEWNAME(OPT.CICS.REPORT.INDEX.FILE)
   ALTER OPT.CICS.NEW.REPORT.INDEX.FILE.*  -
     NEWNAME(OPT.CICS.REPORT.INDEX.FILE.*)

   ALTER OPT.CICS.NEW.REPORT.HISTORY.FILE    -
     NEWNAME(OPT.CICS.REPORT.HISTORY.FILE)
   ALTER OPT.CICS.NEW.REPORT.HISTORY.FILE.*  -
     NEWNAME(OPT.CICS.REPORT.HISTORY.FILE.*)
//
   ALTER OPT.CICS.OLD.REPORT.DETAIL.FILE    -
     NEWNAME(OPT.CICS.REPORT.DETAIL.FILE)
   ALTER OPT.CICS.OLD.REPORT.DETAIL.FILE.*  -
     NEWNAME(OPT.CICS.REPORT.DETAIL.FILE.*)
   ALTER OPT.CICS.OLD.REPORT.INDEX.FILE    -
     NEWNAME(OPT.CICS.REPORT.INDEX.FILE)
   ALTER OPT.CICS.OLD.REPORT.INDEX.FILE.*  -
     NEWNAME(OPT.CICS.REPORT.INDEX.FILE.*)
   ALTER OPT.CICS.OLD.REPORT.HISTORY.FILE    -
     NEWNAME(OPT.CICS.REPORT.HISTORY.FILE)
   ALTER OPT.CICS.OLD.REPORT.HISTORY.FILE.*  -
     NEWNAME(OPT.CICS.REPORT.HISTORY.FILE.*)
//
